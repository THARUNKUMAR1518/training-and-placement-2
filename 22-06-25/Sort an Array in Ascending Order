import java.util.Arrays;

public class ArraySort {
    public static void main(String[] args) {
        int[] nums = {5, 3, 8, 1, 9};
        Arrays.sort(nums);

        System.out.print("Sorted Array: ");
        for (int n : nums) System.out.print(n + " ");
    }
}
Output:
Sorted Array: 1 3 5 8 9
